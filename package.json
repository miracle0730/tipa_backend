{
  "name": "tipa-backend",
  "version": "1.0.0",
  "description": "Backend REST API",
  "main": "api.js",
  "scripts": {
    "api": "node ./build/api.js",
    "api:dev": "./node_modules/.bin/nodemon -e ts --exec ts-node --project tsconfig.json ./src/api.ts",
    "api:master": "./node_modules/.bin/nodemon --config nodemon.master.json -e ts --exec 'ts-node' --project tsconfig.json ./src/api.ts",
    "copy": "copyfiles -u 1 -e './src/**/*.ts' -e './src/*.ts' './src/**/*.*' build",
    "build": "rm -rf ./build && npm run copy && tsc --project tsconfig.json",
    "test": "NODE_ENV=test nodemon --config nodemon.test.json --exec 'jest --runInBand'",
    "test:watch": "NODE_ENV=test nodemon --config nodemon.test.json --exec 'jest --watchAll --runInBand'",
    "~ci:test": "NODE_ENV=test jest --colors --ci",
    "ci:test": "",
    "lint": "tslint --project tsconfig.json",
    "migration": "./node_modules/.bin/nodemon -e ts --exec node migrate.js",
    "migration:master": "./node_modules/.bin/nodemon --config nodemon.master.json -e ts --exec 'node' migrate.js",
    "seed": "./node_modules/.bin/nodemon -e ts --exec 'node' seed.js",
    "seed:master": "./node_modules/.bin/nodemon --config nodemon.master.json -e ts --exec 'node' seed.js"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run build && npm run lint",
      "pre-push": ""
    }
  },
  "author": "Serhii Drahunov<rjhdsy@gmail.com>",
  "license": "ISC",
  "dependencies": {
    "@microsoft/microsoft-graph-client": "^2.2.1",
    "ajv": "^6.10.2",
    "auto-bind": "^2.1.0",
    "aws-sdk": "^2.736.0",
    "axios": "^0.21.0",
    "bcryptjs": "^2.4.3",
    "class-transformer": "^0.2.3",
    "class-validator": "^0.10.2",
    "class-validator-jsonschema": "^1.3.0",
    "generate-password": "^1.5.1",
    "http-graceful-shutdown": "^2.3.2",
    "inversify": "^5.0.1",
    "inversify-inject-decorators": "^3.1.0",
    "isomorphic-fetch": "^3.0.0",
    "jsonwebtoken": "^8.5.1",
    "koa": "^2.8.1",
    "koa-body": "^4.1.1",
    "koa-bodyparser": "^4.2.1",
    "koa-conditional-get": "^2.0.0",
    "koa-convert": "^1.2.0",
    "koa-cors": "0.0.16",
    "koa-etag": "^3.0.0",
    "koa-helmet": "^5.1.0",
    "koa-json": "^2.0.2",
    "koa-router": "^7.4.0",
    "koa-static": "^5.0.0",
    "lodash": "^4.17.15",
    "mailgun-js": "^0.22.0",
    "moment": "^2.24.0",
    "ms": "^2.1.2",
    "perfy": "^1.1.5",
    "pg": "^7.12.1",
    "prom-client": "^11.5.3",
    "qs": "^6.9.4",
    "reflect-metadata": "^0.1.13",
    "reftools": "^1.0.8",
    "routing-controllers": "^0.8.0",
    "routing-controllers-openapi": "^1.7.0",
    "slonik": "^21.4.2",
    "slonik-utilities": "^1.8.2",
    "triple-beam": "^1.3.0",
    "ts-node": "^8.4.1",
    "uuid": "^3.4.0",
    "winston": "^3.2.1",
    "zcrmsdk": "0.0.20"
  },
  "devDependencies": {
    "@slonik/migrator": "^0.2.0",
    "@types/axios": "^0.14.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/core-js": "^2.5.2",
    "@types/jest": "^24.0.18",
    "@types/jsonwebtoken": "^8.3.4",
    "@types/koa": "^2.0.50",
    "@types/koa-conditional-get": "^2.0.0",
    "@types/koa-convert": "^1.2.2",
    "@types/koa-cors": "0.0.0",
    "@types/koa-etag": "^3.0.0",
    "@types/koa-helmet": "^3.1.2",
    "@types/koa-json": "^2.0.18",
    "@types/koa-router": "^7.0.42",
    "@types/koa-static": "^4.0.1",
    "@types/lodash": "^4.14.140",
    "@types/mailgun-js": "^0.22.11",
    "@types/ms": "^0.7.31",
    "@types/node": "^12.11.2",
    "@types/qs": "^6.9.5",
    "@types/slonik": "^21.4.0",
    "@types/triple-beam": "^1.3.0",
    "@types/uuid": "^3.4.5",
    "@types/winston": "^2.4.4",
    "copyfiles": "^2.1.1",
    "husky": "^3.0.5",
