    "openapi-filter": "^1.3.0",
    "speccy": "^0.11.0",
    "supertest": "^4.0.2",
    "swagger-jsdoc": "^3.4.0",
    "ts-jest": "^24.1.0",
    "tslint": "^5.20.0",
    "tslint-consistent-codestyle": "^1.16.0",
    "tslint-sonarts": "^1.9.0",
    "typescript": "^3.7.3"
  }
}
